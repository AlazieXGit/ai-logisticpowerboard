# Environment Configuration
# Copy this file to .env.development or .env.production and fill in your values

# API Configuration
VITE_API_URL=http://localhost:4000

# Node Environment
NODE_ENV=development

# Admin Credentials (use secure values, never commit actual credentials)
VITE_ADMIN_EMAIL=your-admin-email@example.com
VITE_ADMIN_PASSWORD=your-secure-password-here
VITE_BACKUP_EMAIL=your-backup-email@example.com
VITE_BACKUP_PASSWORD=your-secure-backup-password-here
VITE_MASTER_KEY=your-master-key-here
VITE_SYSTEM_ID=your-system-id-here

# Admin Access Codes
VITE_ADMIN_CODE_MASTER=your-master-admin-code
VITE_ADMIN_CODE_SYSTEM=your-system-config-code
VITE_ADMIN_CODE_FEE=your-fee-management-code
VITE_ADMIN_CODE_USER=your-user-management-code
VITE_ADMIN_CODE_DATA=your-data-access-code

# API Keys (never commit real keys)
VITE_SUPABASE_KEY=your-supabase-api-key
VITE_STRIPE_KEY=your-stripe-api-key
VITE_TWILIO_KEY=your-twilio-account-sid
VITE_OPENAI_KEY=your-openai-api-key

# Database URLs (use real connection strings in actual .env files)
VITE_DB_URL_PROD=postgresql://user:password@host:port/database
VITE_DB_URL_STAGING=postgresql://user:password@host:port/database

# DO NOT commit actual .env files with real values to version control
# Always use GitHub Secrets for CI/CD credentials

